{
    "name": "orhanozyalcin/laravel-ticimax",
    "description": "Ticimax Web Service integration for Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTicimax\\Ticimax\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTicimax\\Ticimax\\TicimaxServiceProvider"
            ],
            "aliases": {
                "Ticimax": "LaravelTicimax\\Ticimax\\Facades\\TicimaxFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
